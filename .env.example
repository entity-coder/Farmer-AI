# Global environment variables for Docker Compose
# Copy this file to .env and fill in your values

# Groq API Key (Get free from https://console.groq.com)
GROQ_API_KEY=your_groq_api_key_here

# Node environment
NODE_ENV=production

# Python environment
FLASK_ENV=production

# Ports (usually don't need to change these)
FRONTEND_PORT=3000
BACKEND_PORT=5000
PYTHON_PORT=5001
